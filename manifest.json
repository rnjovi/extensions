{
  "manifest_version": 2,
  "name": "Oikotie Publish date",
  "version": "1.2",
  "description": "A simple Chrome extension for Oikotie asunnot",
  
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
  

  ,"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}